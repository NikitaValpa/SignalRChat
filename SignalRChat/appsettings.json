{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Warning",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SignalR": "Debug",
      "Microsoft.AspNetCore.Http.Connections": "Debug"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SignalRChatContext": "Server=127.0.0.1;port=3306;Database=formamsdispatching;UserId=test;Password=test;"
  },
  "Log4NetCore": {
    "Name": "Test",
    "LoggerRepository": "Fantastic",
    "OverrideCriticalLevelWith": "Fatal",
    "Log4NetConfigFileName": "appsettings.json",
    "Watch": false,
    "UseWebOrAppConfig": true,
    "PropertyOverrides": [
      {
        "XPath": "/log4net/appender[@name='RollingFile']/file",
        "Attributes": {
          "Value": "logs/FormAmsLog.log"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/maximumFileSize",
        "Attributes": {
          "Value": "5MB"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/maxSizeRollBackups",
        "Attributes": {
          "Value": "3"
        }
      },
      {
        "XPath": "/log4net/appender[@name='RollingFile']/file"
      }
    ]
  },
  "Azure": {
    "SignalR": {
      "Enabled": "true"
    }
  }
}